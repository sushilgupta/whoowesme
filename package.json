    "react": "^16.10.1",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "redux": "^4.0.4"
  },
  "devDependencies": {
